<h1>Edit Student</h1>


<%= form_with model: @students do |form| %>
    <div>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
    </div>
  
    <div>
      <%= form.label :age %><br>
      <%= form.text_field :age %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>
